@model ViewModels.HomeIndexViewModel
@{
    ViewBag.Title = "Main";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="text-center m-t-lg">
                <h1>
                    @ViewData["SubTitle"]
                </h1>
                <small>@ViewData["Message"]</small>
            </div>
        </div>
    </div>
</div>
<div class="row  border-bottom white-bg dashboard-header">
    <div class="col-sm-4">
        <h2>Welcome Haydar</h2>
        <small>You have @Model.MpmCount case studies and @Model.MpmResultCount results.</small>
        @Html.Partial("_RecentMpms", Model.RecentMpms)

    </div>
    <div class="col-sm-8">
        @Html.Partial("_LastMpm", Model.LastMpm)

    </div>
</div>
@section Scripts {
    @Html.Partial("_LastMpmScripts", Model.LastMpm)
}